<p id="notice"><%= notice %></p>

<h1>Food Weeks</h1>

<table>
  <thead>
    <tr>
      <th>Week</th>
      <th>Day</th>
      <th>Soup1</th>
      <th>Soup2</th>
      <th>Food1</th>
      <th>Food2</th>
      <th>Food3</th>
      <th>Food4</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @food_weeks.each do |food_week| %>
      <tr>
        <td><%= food_week.week_id %></td>
        <td><%= food_week.day_id %></td>
        <td><%= food_week.soup1_id %></td>
        <td><%= food_week.soup2_id %></td>
        <td><%= food_week.food1_id %></td>
        <td><%= food_week.food2_id %></td>
        <td><%= food_week.food3_id %></td>
        <td><%= food_week.food4_id %></td>
        <td><%= link_to 'Show', food_week %></td>
        <td><%= link_to 'Edit', edit_food_week_path(food_week) %></td>
        <td><%= link_to 'Destroy', food_week, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Food Week', new_food_week_path %>
